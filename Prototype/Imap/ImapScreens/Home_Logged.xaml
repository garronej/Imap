<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:pi="http://schemas.microsoft.com/prototyping/2010/interactivity"
	xmlns:local="clr-namespace:ImapScreens"
	mc:Ignorable="d"
	x:Class="ImapScreens.Home_Logged"
	Width="640" Height="1130">

	<Grid x:Name="LayoutRoot" Background="White">
		<Grid Margin="10,787,10,10">
			<Button Content="Full Schedule(ADE)" HorizontalAlignment="Left" Height="58" Margin="10,231,0,0" VerticalAlignment="Top" Width="281">
				<i:Interaction.Triggers>
					<i:EventTrigger EventName="Click">
						<pi:NavigateToScreenAction TargetScreen="ImapScreens.Google_Calendar"/>
					</i:EventTrigger>
				</i:Interaction.Triggers>
			</Button>
			<Button Content="Info About your university" HorizontalAlignment="Left" Height="58" Margin="348,231,0,0" VerticalAlignment="Top" Width="272">
				<i:Interaction.Triggers>
					<i:EventTrigger EventName="Click">
						<pi:NavigateToScreenAction TargetScreen="ImapScreens.School_data"/>
					</i:EventTrigger>
				</i:Interaction.Triggers>
			</Button>
			<Button Content="See on the map" HorizontalAlignment="Left" Height="180" Margin="436,10,0,0" VerticalAlignment="Top" Width="169">
				<i:Interaction.Triggers>
					<i:EventTrigger EventName="Click">
						<pi:NavigateToScreenAction TargetScreen="ImapScreens.Map"/>
					</i:EventTrigger>
				</i:Interaction.Triggers>
			</Button>
			<TextBlock HorizontalAlignment="Left" Height="180" Margin="10,10,0,0" TextWrapping="Wrap" Text="NEXT:" VerticalAlignment="Top" Width="426"/>
		</Grid>
		<Grid HorizontalAlignment="Left" Height="136" Margin="0,518,0,0" VerticalAlignment="Top" Width="620">
			<Button Content="More" HorizontalAlignment="Left" Height="116" Margin="472,10,0,0" VerticalAlignment="Top" Width="138"/>
		</Grid>
		<local:LayoutRoot HorizontalAlignment="Left" Margin="0,0,0,-6" VerticalAlignment="Top" d:IsPrototypingComposition="True"/>
		<local:Search_component HorizontalAlignment="Left" VerticalAlignment="Top" d:IsPrototypingComposition="True"/>
	</Grid>
</UserControl>