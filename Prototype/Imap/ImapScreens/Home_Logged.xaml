		<Grid HorizontalAlignment="Left" Height="256" Margin="0,864,0,0" VerticalAlignment="Top" Width="640">
			<Button Content="Full Schedule(ADE)" HorizontalAlignment="Left" Height="72" Margin="10,174,0,0" VerticalAlignment="Top" Width="301"/>
			<Button Content="Information on your university" HorizontalAlignment="Left" Height="72" Margin="334,174,0,0" VerticalAlignment="Top" Width="296"/>
			<Button Content="See on the map" HorizontalAlignment="Left" Height="159" Margin="470,10,0,0" VerticalAlignment="Top" Width="160"/>
			<TextBox HorizontalAlignment="Left" Height="159" Margin="10,10,0,0" TextWrapping="Wrap" Text="Next" VerticalAlignment="Top" Width="455"/>
		</Grid>
	</Grid>
</UserControl>