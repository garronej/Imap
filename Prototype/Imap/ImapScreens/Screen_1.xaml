			<TextBlock HorizontalAlignment="Left" Margin="203,79,0,0" Style="{StaticResource BasicTextBlock-Sketch}" TextWrapping="Wrap" Text="Or" VerticalAlignment="Top" FontFamily="/Imap.Screens;component/Fonts/Fonts.zip#Microsoft JhengHei Light" FontSize="21.333"/>
			<Button Content="Custom search" HorizontalAlignment="Left" Height="58" Margin="10,156,0,0" VerticalAlignment="Top" Width="176" FontSize="21.333" FontFamily="/Imap.Screens;component/Fonts/Fonts.zip#Microsoft JhengHei Light"/>
			<Button Content="Go map" HorizontalAlignment="Left" Height="58" Margin="253,156,0,0" VerticalAlignment="Top" Width="155" FontFamily="Microsoft JhengHei Light" FontSize="21.333"/>
		</Grid>
	</Grid>
</UserControl>