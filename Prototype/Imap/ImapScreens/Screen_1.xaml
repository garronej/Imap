<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
	mc:Ignorable="d"
	x:Class="ImapScreens.Screen_1"
	Width="640" Height="1129.9">

	<Grid x:Name="LayoutRoot" Background="White">
		<Grid HorizontalAlignment="Left" Height="224" Margin="104,233,0,0" VerticalAlignment="Top" Width="418">
			<Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="224" Stroke="Black" VerticalAlignment="Top" Width="418"/>
